{
  "parser": "babel-eslint",

  "env": {
      "browser": true,
      "jasmine": true
  },

  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
  ],

  "globals": {
    "jest": true,
  },

  "plugins": [
    "react",
    "react-native"
  ],

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",

    "ecmaFeatures": {
      " jsx": true
    }
  }
}
